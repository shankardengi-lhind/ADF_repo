{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_fmg_copy_pl_sink_02",
						"type": "DatasetReference"
					},
					"name": "fmgfactsource01"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_fmg_df_fact_sink03",
						"type": "DatasetReference"
					},
					"name": "dffmgsink04"
				}
			],
			"transformations": [
				{
					"name": "slfmgselect02"
				},
				{
					"name": "splitcountry"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> fmgfactsource01",
				"fmgfactsource01 select(mapColumn(",
				"          InvoiceNo,",
				"          StockCode,",
				"          Description,",
				"          Quantity,",
				"          InvoiceDate,",
				"          CustomerID,",
				"          Country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> slfmgselect02",
				"slfmgselect02 split(Country==\"Germany\",",
				"     Country==\"United Kingdom\",",
				"     disjoint: false) ~> splitcountry@(countrygermany, UnitedKingdom, othercountry)",
				"splitcountry@countrygermany sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> dffmgsink04"
			]
		}
	}
}