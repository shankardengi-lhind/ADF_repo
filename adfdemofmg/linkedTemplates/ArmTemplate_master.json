{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemofmg"
		},
		"ls_sql_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_source'"
		},
		"ls_stgen2_source_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_stgen2_source'"
		},
		"ls_stgen2_target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_stgen2_target'"
		},
		"ls_github_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"ls_poke_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co"
		},
		"ls_sql_source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "fmg-demo-server.database.windows.net"
		},
		"ls_sql_source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "fmg_demo"
		},
		"ls_sql_source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shanadmin"
		},
		"ls_stgen2_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemofmg.dfs.core.windows.net/"
		},
		"ls_stgen2_target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemofmg.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemofmg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_source_password": {
						"value": "[parameters('ls_sql_source_password')]"
					},
					"ls_stgen2_source_accountKey": {
						"value": "[parameters('ls_stgen2_source_accountKey')]"
					},
					"ls_stgen2_target_accountKey": {
						"value": "[parameters('ls_stgen2_target_accountKey')]"
					},
					"ls_github_source_properties_typeProperties_url": {
						"value": "[parameters('ls_github_source_properties_typeProperties_url')]"
					},
					"ls_poke_api_properties_typeProperties_url": {
						"value": "[parameters('ls_poke_api_properties_typeProperties_url')]"
					},
					"ls_sql_source_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_source_properties_typeProperties_server')]"
					},
					"ls_sql_source_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_source_properties_typeProperties_database')]"
					},
					"ls_sql_source_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_source_properties_typeProperties_userName')]"
					},
					"ls_stgen2_source_properties_typeProperties_url": {
						"value": "[parameters('ls_stgen2_source_properties_typeProperties_url')]"
					},
					"ls_stgen2_target_properties_typeProperties_url": {
						"value": "[parameters('ls_stgen2_target_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdemofmg_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfdemofmg_ArmTemplate_0')]"
			]
		}
	]
}